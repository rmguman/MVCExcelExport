@{
    ViewBag.Title = "Home Page";
}

@model List<ExcelExample.Models.UserVM>

<h1>Excel</h1>
<hr />
<div>
    <a href="@Url.Action("DownloadExcel","Home")">Download Excel</a>
</div>
<hr />

<table class="table table-dark">
    <thead>
        <tr>
            <th scope="col">ID</th>
            <th scope="col">Name</th>
            <th scope="col">Surname</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.ID</td>
                <td>@item.Name</td>
                <td>@item.Surname</td>
            </tr>
        }
    </tbody>
</table>

